function GetFastUrl() {
  # �Ѵ����url����list
  local url_list=$*
  # ����һ����ʱ����ʱֵ
  local time=10.0
  # ����һ��url��ֵ
  local url=""
  # ��ʼ����url
  for item in ${url_list[*]} ; do
    # ��ȡurl�ķ����ٶ�
    local itime=`curl -o /dev/null -L -s -w '%{time_starttransfer}' $item`
    #echo $item "access time is:" $itime
    # �ж���������ٶ��Ƿ��ǿ��,����Ǳȳ�ʱ��ֵ��ʹ�����.
    if [[ `expr $itime \> $time` -eq 0 ]]  ; then
      url=$item
      time=$itime
    fi
  done
  echo $url
}

url=$(GetFastUrl ${YUM_URLS[*]})
echo $url

